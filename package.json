{
  "name": "uswds-react",
  "version": "0.0.1",
  "description": "U.S. Web Design Standard components written in React.",
  "scripts": {
    "start": "cross-env NODE_PATH=./styleguide concurrently --kill-others \"nodemon --watch . --ext js,jsx server\" \"node support/webpack.server.js\" \"gulp css:watch\"",
    "lint": "eslint . --ext .jsx --ext .js",
    "test": "jest",
    "test:ci": "cross-env NODE_ENV=test cross-env NODE_PATH=./styleguide mocha --compilers js:babel-register -r app/spec/support/setup.mocha.js --recursive 'app/spec/**/*.test.js'",
    "webpack": "webpack --config support/webpack.config.js",
    "build": "rimraf dist/ && npm run webpack && gulp build",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "6.1.0",
    "npm": "3.8.6"
  },
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.5.1",
    "bluebird": "^2.10.0",
    "bourbon": "^4.2.6",
    "bourbon-neat": "github:thoughtbot/neat#neat-1.8.0-node-sass",
    "compression": "^1.6.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.0",
    "express": "^4.15.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-filter": "^3.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^6.0.1",
    "gulp-rev-all": "^0.8.24",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "history": "^1.17.0",
    "humps": "^0.6.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "normalizr": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.0",
    "react-highlight": "^0.9.0", 
    "react-router": "^3.0.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.8.0",
    "redux-thunk": "^2.2.0",
    "rewire": "^2.3.4",
    "rimraf": "^2.6.0",
    "run-sequence": "^1.2.2",
    "superagent": "^1.4.0",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "jsdom": "^7.0.1",
    "mocha": "^2.4.5",
    "nock": "^2.17.0",
    "nodemon": "^1.6.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.2",
    "rewire-webpack": "^1.0.0",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "webpack-dev-server": "^2.4.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!gulpfile.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/server/**",
      "!**/styleguide/**",
      "!**/support/**"
    ]
  }
}
